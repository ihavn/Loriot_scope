<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="org.edu.via.sep4.lorawan_scope.view.uplink_message.UplinkMessageController">
    <VBox alignment="CENTER" spacing="20.0">
        <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
        </padding>
        <Label text="Uplink Data Received by LoRaWAN">
            <font>
                <Font name="System Bold" size="24.0"/>
            </font>
        </Label>
        <TableView fx:id="uplinkDataTable" prefHeight="500.0" prefWidth="1000.0">
            <columns>
                <TableColumn fx:id="devEUICol" prefWidth="200.0" text="Device EUI"/>
                <TableColumn fx:id="localTimeCol" prefWidth="200.0" text="Local time"/>
                <TableColumn fx:id="portCol" prefWidth="75.0" style="-fx-alignment: center;" text="Port"/>
                <TableColumn fx:id="fcntUpCol" prefWidth="75.0" style="-fx-alignment: center;" text="FCntUp"/>
                <TableColumn fx:id="payloadCol" prefWidth="400.0" text="Payload"/>
            </columns>
        </TableView>
    </VBox>
</AnchorPane>
